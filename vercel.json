{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "frontend/expence-tracker/package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*\\..*)", "dest": "/frontend/expence-tracker/dist/$1" },
    { "src": "/(.*)", "dest": "/frontend/expence-tracker/dist/index.html" }
  ]
}
